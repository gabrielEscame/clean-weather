{
  "name": "clean-weather",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Gabriel Escame <gabriel.escame@gmail.com>",
  "license": "MIT",
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{ts,tsx}",
      "!**/*.d.ts"
    ],
    "coverageDirectory": "coverage",
    "testEnvironment": "jsdom",
    "transform": {
      ".+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/src/$1",
      "\\.scss$": "indentity-obj-proxy"
    }
  },
  "scripts": {
    "start": "webpack-dev-server --open",
    "test": "jest --no-cache"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.2.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.2",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.11",
    "jest": "^29.2.2",
    "node-sass": "^8.0.0",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
